# Environment variables for DentalAI
# Copy this file to `.env` and fill in real secrets before running.

OPENAI_API_KEY=your-openai-api-key
LANGCHAIN_API_KEY=your-langsmith-api-key
LANGCHAIN_PROJECT=dental_v1
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

TAVILY_API_KEY=your-tavily-api-key
MISTRAL_API_KEY=your-mistral-api-key

# Identify your requests to external services
USER_AGENT="DentalAI/0.1 (+https://example.com/contact)"
